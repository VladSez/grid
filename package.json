{
  "name": "@supabase/grid",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/supabase-grid.esm.js",
  "size-limit": [
    {
      "path": "dist/supabase-grid.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/supabase-grid.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@tailwindcss/forms": "^0.3.2",
    "@types/file-saver": "^2.0.2",
    "@types/react": "^16.9.11",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "direction": "^1.0.4",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "marked": "^0.7.0",
    "postcss": "^8.2.7",
    "react": "^16.12 || ^17.0",
    "react-dom": "^16.12 || ^17.0",
    "react-responsive-carousel": "^3.2.12",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.9.2",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.1.3",
    "@supabase/react-data-grid": "7.1.0-canary.1",
    "@supabase/supabase-js": "^1.11.15",
    "@supabase/ui": "^0.26.6",
    "awesome-debounce-promise": "^2.1.0",
    "dayjs": "^1.10.4",
    "file-saver": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "p-queue": "6.6.2",
    "react-dnd": "^14.0.1",
    "react-dnd-html5-backend": "^14.0.0",
    "react-tiny-popover": "^6.0.5",
    "react-tracked": "^1.7.1",
    "scheduler": "^0.20.2"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
